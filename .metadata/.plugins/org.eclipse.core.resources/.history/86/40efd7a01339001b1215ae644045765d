<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper_oracle.CategoryMapper">
<insert id="categoryinsert">
	insert into category(categorycode, categoryname) values(#{categorycode}, #{categoryname})
</insert>
<select id="categorylist" resultType="hashmap">
	select (substr(categoryname,1,INSTR(categoryname,'/',1,2)-1)) name 
	from category 
	where categoryname like '골드스탁/'||'%'||'/empty' 
	group by substr(categoryname,1,INSTR(categoryname,'/',1,2)-1)
</select>
<select id="list" resultType="hashmap">
	select * from company
</select>
</mapper>